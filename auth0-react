## Install node.js
curl -sL https://deb.nodesource.com/setup_4.x | sudo -E bash -
sudo apt-get install -y nodejs

## For newer node version
## curl -sL https://deb.nodesource.com/setup_6.x | sudo -E bash -
## sudo apt-get install -y nodejs

## Not needed in Ubuntu
## sudo apt-get install -y git


## forked-from repo git clone https://github.com/auth0-samples/auth0-react-sample.git
https://github.com/DNAvid/auth0-react-sample.git
cd auth0-react-sample/01-Login/

# Install the dependencies
npm install

# copy configuration (see below)
cp .env.example .env

# Run
npm start
